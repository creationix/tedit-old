{
  "name": "Tim's Editor",
  "version": "0.0.1",
  "manifest_version": 2,
  "offline_enabled": true,
  "description": "Edit Git Repositories",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*",
    {"socket": [ "tcp-connect:*:*" ]}
  ]
}
